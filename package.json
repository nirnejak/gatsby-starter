{
  "name": "gatsby-starter",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Jitendra Nirnejak <jeetnirnejak@gmail.com>",
  "dependencies": {
    "gatsby": "^3.6.2",
    "gatsby-plugin-canonical-urls": "^3.6.0",
